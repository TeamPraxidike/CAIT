{
	"name": "ml-teachers",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"setup": "npm install && npx prisma generate",
		"dev": "chmod +x scripts/createSAMLidp.sh && scripts/dev.sh",
		"prebuild": "chmod +x scripts/dirname.sh && chmod +x scripts/createSAMLidp.sh && npx prisma generate && prisma migrate deploy",
		"build": "npm run prebuild && scripts/createSAMLidp.sh && vite build && scripts/dirname.sh",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"test:unit": "vitest --coverage -c ./vitest.config.unit.ts",
		"test:integration": "scripts/run-integration.sh",
		"test:e2e": "scripts/run-e2e.sh",
		"test:e2e:report": "scripts/run-e2e.sh && playwright show-report",
		"test:e2e:generator": "npx playwright codegen localhost:5173/"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.9.1",
		"@esbuild-plugins/tsconfig-paths": "^0.1.2",
		"@eslint/js": "^9.1.1",
		"@iconify/svelte": "^4.0.2",
		"@iconify/tailwind": "^1.1.1",
		"@playwright/test": "^1.56.1",
		"@prisma/client": "^5.22.0",
		"@skeletonlabs/skeleton": "2.9.2",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/enhanced-img": "^0.2.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/svelte": "^5.1.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.14.2",
		"@types/workerpool": "^6.4.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-istanbul": "^3.1.2",
		"@vitest/coverage-v8": "^3.1.2",
		"autoprefixer": "10.4.19",
		"esbuild": "^0.25.9",
		"eslint": "^8.57.0",
		"eslint-plugin-svelte": "^2.35.1",
		"globals": "^15.1.0",
		"jsdom": "^24.0.0",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prisma": "^5.22.0",
		"rollup-plugin-copy": "^3.5.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^7.8.0",
		"vite": "5.4.21",
